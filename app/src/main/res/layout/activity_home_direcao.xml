<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/fundo_tela_principal"
    tools:context=".home_direcao">

  <TextView
      android:id="@+id/textView6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="20dp"
      android:text="Bom Dia Diretor Michael"
      android:textColor="@color/white"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <TextView
      android:id="@+id/textView7"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="10dp"
      android:text="Hoje é dia 03/09/2024"
      android:textColor="@color/white"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView6" />

  <ImageView
      android:id="@+id/btnAgenda"
      android:layout_width="90dp"
      android:layout_height="90dp"
      android:layout_marginStart="100dp"
      android:layout_marginTop="72dp"
      android:clickable="true"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView7"
      app:srcCompat="@drawable/btn_diretor_agenda" />

  <ImageView
      android:id="@+id/btnEnviarMaterail"
      android:layout_width="90dp"
      android:layout_height="90dp"
      android:layout_marginStart="20dp"
      android:layout_marginTop="72dp"
      android:layout_marginEnd="100dp"
      android:clickable="true"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/btnAgenda"
      app:layout_constraintTop_toBottomOf="@+id/textView7"
      app:srcCompat="@drawable/btn_diretor_material" />

  <ImageView
      android:id="@+id/btnAvisos"
      android:layout_width="90dp"
      android:layout_height="90dp"
      android:layout_marginStart="100dp"
      android:layout_marginTop="20dp"
      android:clickable="true"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/btnAgenda"
      app:srcCompat="@drawable/btn_diretor_avisos" />

  <ImageView
      android:id="@+id/btnLobitos"
      android:layout_width="90dp"
      android:layout_height="90dp"
      android:layout_marginTop="20dp"
      android:layout_marginEnd="100dp"
      android:clickable="true"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/btnMensalidades"
      app:srcCompat="@drawable/btn_diretor_lobitos" />

  <ImageView
      android:id="@+id/btnMensalidades"
      android:layout_width="90dp"
      android:layout_height="90dp"
      android:layout_marginStart="20dp"
      android:layout_marginTop="20dp"
      android:layout_marginEnd="100dp"
      android:clickable="true"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/btnAvisos"
      app:layout_constraintTop_toBottomOf="@+id/btnEnviarMaterail"
      app:srcCompat="@drawable/btn_diretor_mensalidade" />

  <ImageView
      android:id="@+id/btnPreenca"
      android:layout_width="90dp"
      android:layout_height="90dp"
      android:layout_marginStart="20dp"
      android:layout_marginEnd="100dp"
      android:clickable="true"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toEndOf="@+id/btnMensalidades"
      app:srcCompat="@drawable/btn_diretor_presenca"
      tools:layout_editor_absoluteY="462dp" />

  <TextView
      android:id="@+id/textView8"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="5dp"
      android:text="Agenda"
      android:textColor="@color/letras_tela_principal"
      app:layout_constraintBottom_toBottomOf="@+id/btnAgenda"
      app:layout_constraintEnd_toEndOf="@+id/btnAgenda"
      app:layout_constraintStart_toStartOf="@+id/btnAgenda" />

  <TextView
      android:id="@+id/textView9"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="5dp"
      android:text="Enviar Material"
      android:textColor="@color/letras_tela_principal"
      android:textSize="12dp"
      app:layout_constraintBottom_toBottomOf="@+id/btnEnviarMaterail"
      app:layout_constraintEnd_toEndOf="@+id/btnEnviarMaterail"
      app:layout_constraintStart_toStartOf="@+id/btnEnviarMaterail" />

  <TextView
      android:id="@+id/textView10"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="5dp"
      android:text="Avisos"
      android:textColor="@color/letras_tela_principal"
      app:layout_constraintBottom_toBottomOf="@+id/btnAvisos"
      app:layout_constraintEnd_toEndOf="@+id/btnAvisos"
      app:layout_constraintStart_toStartOf="@+id/btnAvisos" />

  <TextView
      android:id="@+id/textView11"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="5dp"
      android:text="Lobitos"
      android:textColor="@color/letras_tela_principal"
      app:layout_constraintBottom_toBottomOf="@+id/btnLobitos"
      app:layout_constraintEnd_toEndOf="@+id/btnLobitos"
      app:layout_constraintStart_toStartOf="@+id/btnLobitos" />

  <TextView
      android:id="@+id/textView12"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="5dp"
      android:text="Menalidade"
      android:textColor="@color/letras_tela_principal"
      app:layout_constraintBottom_toBottomOf="@+id/btnMensalidades"
      app:layout_constraintEnd_toEndOf="@+id/btnMensalidades"
      app:layout_constraintStart_toStartOf="@+id/btnMensalidades" />

  <ImageView
      android:id="@+id/btnConfiguracoes"
      android:layout_width="90dp"
      android:layout_height="90dp"
      android:clickable="true"
      app:srcCompat="@drawable/engrenagem"
      tools:layout_editor_absoluteX="221dp"
      tools:layout_editor_absoluteY="473dp" />

  <TextView
      android:id="@+id/textView14"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="5dp"
      android:maxLines="2"
      android:text="Configuraçoes"
      android:textColor="@color/letras_tela_principal"
      android:textSize="12dp"
      app:layout_constraintBottom_toBottomOf="@+id/btnConfiguracoes"
      app:layout_constraintEnd_toEndOf="@+id/btnConfiguracoes"
      app:layout_constraintStart_toStartOf="@+id/btnConfiguracoes" />

  <ImageView
      android:id="@+id/btnsair"
      android:layout_width="33dp"
      android:layout_height="34dp"
      android:layout_marginTop="10dp"
      android:layout_marginEnd="15dp"
      android:clickable="true"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/air" />

  <ImageView
      android:id="@+id/imageView6"
      android:layout_width="90dp"
      android:layout_height="90dp"
      android:layout_marginStart="100dp"
      android:layout_marginTop="20dp"
      android:clickable="true"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/btnAvisos"
      app:srcCompat="@drawable/btn_diretor_potuacao" />

  <ImageView
      android:id="@+id/imageView10"
      android:layout_width="90dp"
      android:layout_height="90dp"
      android:layout_marginStart="100dp"
      android:layout_marginTop="20dp"
      android:clickable="true"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView6"
      app:srcCompat="@drawable/desbravador" />

  <TextView
      android:id="@+id/textView15"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="5dp"
      android:maxLines="2"
      android:text="Pontuação"
      android:textColor="@color/letras_tela_principal"
      android:textSize="12dp"
      app:layout_constraintBottom_toBottomOf="@+id/imageView6"
      app:layout_constraintEnd_toEndOf="@+id/imageView6"
      app:layout_constraintStart_toStartOf="@+id/imageView6" />

  <TextView
      android:id="@+id/textView16"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="5dp"
      android:maxLines="2"
      android:text="Desbravador"
      android:textColor="@color/letras_tela_principal"
      android:textSize="12dp"
      app:layout_constraintBottom_toBottomOf="@+id/imageView10"
      app:layout_constraintEnd_toEndOf="@+id/imageView10"
      app:layout_constraintStart_toStartOf="@+id/imageView10" />

</androidx.constraintlayout.widget.ConstraintLayout>